docker network create se

docker run -d -p 3306:3306 --name se-db --network se -e MYSQL_ROOT_PASSWORD=SEP@ssw0rd se-db:rc-01

docker run -d --name statusengine --network se -p 4730:4730 -v "$(pwd)"/naemon.cfg:/opt/naemon/etc/naemon/naemon.cfg -v "$(pwd)"/config.yml:/opt/statusengine/worker/etc/config.yml statusengine:rc-01

docker run -d -p 80:80 -p 443:443 --name se-ui --network se se-ui:rc-01

docker exec -t se-ui /usr/share/statusengine-ui/bin/Console.php users add --username "admin" --password "admin"
